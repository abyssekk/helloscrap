<%= search_form_for @q, url: url do |f| %>
    <div class="input-group">
        <%= f.search_field :title_or_body_or_notice_cont, class: 'form-control', placeholder: '検索ワード' %>
        <div class="input-group-append">
            <%= f.submit '検索', class: 'btn btn-primary' %>
        </div>
    </div>
<% end %> 